{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "npm run build",
  "devCommand": "npm run start",
  "framework": "angular",
  "outputDirectory": "dist/browser",
  "redirects": [
    {
      "source": "/",
      "destination": "/pt-BR",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/pt-BR",
      "headers": [
        {
          "key": "Location",
          "value": "/pt-BR"
        }
      ]
    },
    {
      "source": "/en-US",
      "headers": [
        {
          "key": "Location",
          "value": "/en-US"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/pt-BR/(.*)",
      "destination": "/pt-BR/index.html"
    },
    {
      "source": "/en-US/(.*)",
      "destination": "/en-US/index.html"
    }
  ]
}